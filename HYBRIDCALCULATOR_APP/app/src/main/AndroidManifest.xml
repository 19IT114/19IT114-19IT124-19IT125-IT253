<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hybridcalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Ressistance"></activity>
        <activity android:name=".Day" />
        <activity android:name=".Programmer" />
        <activity android:name=".WeightandMass" />
        <activity android:name=".Pressure" />
        <activity android:name=".Data" />
        <activity android:name=".Energy" />
        <activity android:name=".Volume" />
        <activity android:name=".Area" />
        <activity android:name=".Time" />
        <activity android:name=".Speed" />
        <activity android:name=".Angle" />
        <activity android:name=".Power" />
        <activity android:name=".Length" />
        <activity android:name=".Temperature" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>